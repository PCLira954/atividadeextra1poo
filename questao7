class Retangulo {
    l1: number = 0;
    l2: number = 0;

    calcularArea(): number{
        return this.l1 * this.l2;
    }

    calcularPerimetro(): number{
        return this.l1 + this.l2;
    }
}

let retangul0 : Retangulo;
retangul0 = new Retangulo();

retangul0.l1 = 10;
retangul0.l2 = 20;

console.log(retangul0.calcularArea());
console.log(retangul0.calcularPerimetro());
