class SituacaoFinanceira{
    valorCreditos: number = 0;
    valorDebitos: number = 0;

    calcularSaldo(): number{
        return this.valorCreditos - this.valorDebitos;
    }

}

let suasituacaofinanceira : SituacaoFinanceira;
suasituacaofinanceira = new SituacaoFinanceira();

suasituacaofinanceira.valorCreditos = 1000;
suasituacaofinanceira.valorDebitos = 400;

console.log(suasituacaofinanceira.calcularSaldo());
